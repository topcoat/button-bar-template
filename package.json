{
  "name": "topcoat-button-bar-template",
  "version": "0.1.0",
  "description": "Template for Topcoat button bar",
  "main": "index.js",
  "scripts": {
    "test": "tap test/test.js",
    "build-js": "browserify ./example/main.js > ./example/bundle.js",
    "build-css": "resinate -s ./example/entry.css -v true -e true -n topcoat > ./example/bundle.css",
    "build": "npm run build-js && npm run build-css"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/topcoat/button-bar-template.git"
  },
  "keywords": [
    "template",
    "topcoat",
    "button",
    "bar"
  ],
  "author": "Kristofer Joseph @dam",
  "license": "Apache2",
  "bugs": {
    "url": "https://github.com/topcoat/button-bar-template/issues"
  },
  "homepage": "https://github.com/topcoat/button-bar-template",
  "devDependencies": {
    "tap": "^0.4.8",
    "mocha": "^1.17.1",
    "topcoat-theme-mobile-light": "^0.3.0",
    "topcoat-button-bar": "^0.2.1",
    "resinate": "^0.1.0",
    "browserify": "^3.32.0",
    "topcoat-theme-mobile-dark": "^0.3.0"
  },
  "dependencies": {
    "lodash-node": "^2.4.1"
  }
}
